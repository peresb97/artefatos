}

se(paramTurma != nulo) {
	fonteTurmaFuncionario = Dados.profesores.v1.turmasFuncionarios.lista(campos: "turma.id, turma.modalidadeTurma", criterio: "id = ${paramTurma}")
	turID = fonteTurmaFuncionario[0].turma.id
